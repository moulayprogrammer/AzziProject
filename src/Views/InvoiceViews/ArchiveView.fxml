<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<DialogPane nodeOrientation="RIGHT_TO_LEFT" prefHeight="600.0" prefWidth="1000.0" style="-fx-background-color: #FFF;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ReceiptMedicationControllers.ArchiveController">
   <content>
      <BorderPane>
         <center>
            <VBox prefHeight="400.0" prefWidth="600.0">
               <children>
                   <AnchorPane prefHeight="50.0" prefWidth="600.0" style="-fx-background-color: #FFF;">
                       <children>
                           <ImageView fitHeight="26.0" fitWidth="29.0" layoutX="640.0" layoutY="11.0" onMousePressed="#ActionRefresh" pickOnBounds="true" preserveRatio="true">
                               <image>
                                   <Image url="@../../Icons/refresh.jpeg" />
                               </image>
                           </ImageView>
                           <ImageView fitHeight="29.0" fitWidth="35.0" layoutX="594.0" layoutY="9.0" onMousePressed="#ActionSearch" pickOnBounds="true" preserveRatio="true">
                               <image>
                                   <Image url="@../../Icons/recherche.png.png" />
                               </image>
                           </ImageView>
                           <TextField fx:id="tfRecherche" layoutX="1.0" layoutY="8.0" prefHeight="30.0" prefWidth="564.0">
                               <font>
                                   <Font name="System Bold" size="13.0" />
                               </font>
                           </TextField>
                       </children>
                   </AnchorPane>
                  <TableView fx:id="table" minHeight="450.0">
                     <columns>
                        <TableColumn fx:id="clId" text="المعرف" />
                        <TableColumn fx:id="clProvider" prefWidth="250.0" text="المورد" />
                        <TableColumn fx:id="clDate" prefWidth="140.0" text="تاريخ الشراء" />
                        <TableColumn fx:id="clAmount" prefWidth="180.0" text="مبلغ الفاتورة" />
                        <TableColumn fx:id="clPaying" prefWidth="180.0" text="المبلغ المسدد" />
                        <TableColumn fx:id="clDebt" prefWidth="180.0" text="المبلغ المدين" />
                     </columns>
                  </TableView>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </content>
   <header>
      <AnchorPane prefHeight="60.0" prefWidth="100.0" style="-fx-background-color: #EFF;">
         <children>
            <Button fx:id="btnInsert" layoutX="645.0" layoutY="13.0" mnemonicParsing="false" onMousePressed="#ActionDeleteFromArchive" prefHeight="34.0" prefWidth="163.0" style="-fx-background-color: #FFF; -fx-border-color: #2005a8;" text="الغاء الارشفة" textFill="#2005a8" AnchorPane.rightAnchor="192.0">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="22.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Icons/Save.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button layoutX="833.0" layoutY="13.0" mnemonicParsing="false" onMousePressed="#ActionAnnuler" prefHeight="32.0" prefWidth="144.0" style="-fx-background-color: #FFF; -fx-border-color: #dd0000;" text="الغاء" textFill="#dd0000">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="22.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Icons/delete.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Label layoutX="14.0" layoutY="19.0" text="الغاء ارشفة الأدوية">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </header>
</DialogPane>
